<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="titan.wms.common.dao.mapper.basicInfo.PackingMaterialParseRuleItemMapper" >
    <resultMap id="BaseResult" type="PackingMaterialParseRuleItemDO" >
        <result column="id" property="id"/>
        <result column="rule_id" property="ruleId"/>
        <result column="begin_pos" property="beginPos"/>
        <result column="end_pos" property="endPos"/>
        <result column="is_code" property="code"/>
        <result column="remark" property="remark"/>
        <result column="create_time" property="createTime"/>
        <result column="update_time" property="updateTime"/>
    </resultMap>

    <sql id="TableName">
        outbound_packing_material_parse_rule_item
    </sql>

    <sql id="BaseColumn" >
        `rule_id`,
        `begin_pos`,
        `end_pos`,
        `is_code`,
        `remark`,
        `create_time`,
        `update_time`
    </sql>

    <insert id="insert" parameterType="PackingMaterialParseRuleItemDO" keyProperty="id" useGeneratedKeys="true">
        INSERT INTO
          <include refid="TableName" /> (<include refid="BaseColumn" />)
        VALUES(
          #{ruleId},
          #{beginPos},
          #{endPos},
          #{code},
          #{remark},
          NOW(),
          NOW()
        )
    </insert>

    <select id="getById" resultMap="BaseResult">
        SELECT
          id, <include refid="BaseColumn" />
        FROM
          <include refid="TableName"/>
        WHERE
          id = #{id}
    </select>

    <select id="getByRuleId" resultMap="BaseResult">
        SELECT
          id, <include refid="BaseColumn" />
        FROM
          <include refid="TableName"/>
        WHERE
          rule_id = #{ruleId}
    </select>

    <delete id="deleteById">
        DELETE
        FROM
          <include refid="TableName"/>
        WHERE
          id = #{id}
    </delete>

    <delete id="deleteByRuleIds">
        DELETE
        FROM
          <include refid="TableName"/>
        WHERE rule_id IN
        <foreach collection="ruleIds" item="ruleId" index="index" open="(" separator="," close=")">
          #{ruleId}
        </foreach>
    </delete>

    <insert id="batchInsert" parameterType="PackingMaterialParseRuleItemDO">
        INSERT INTO
          <include refid="TableName"/> (<include refid="BaseColumn" />)
        VALUES
        <foreach collection="list" item="item" index="index" separator=",">
        (
          #{item.ruleId},
          #{item.beginPos},
          #{item.endPos},
          #{item.code},
          #{item.remark},
          NOW(),
          NOW()
        )
        </foreach>
    </insert>
</mapper>