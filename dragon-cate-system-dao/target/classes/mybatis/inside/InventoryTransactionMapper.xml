<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "classpath:dtd/mybatis-3-mapper.dtd" >
<mapper namespace="titan.wms.common.dao.mapper.inside.InventoryTransactionMapper">

	<sql id="TABLE_NAME">
		`inside_inventory_transaction_history`
	</sql>

	<sql id="INSERT_ALL_COLUMNS">
		`sku_code`, `attribute_id`, `location_code`, `container_code`, `client_code`
		,`transaction_type`,`sub_type`,`source_order_no`,`source_order_line`,`source_order_type`,`source_task_no`,`source_task_line`,`reason`
		, `change_type`, `count`, `before_count`, `after_count`, `operator_id`,`create_time`
	</sql>

	<sql id="SELECT_ALL_COLUMNS">
		`id`,`create_time`,
		<include refid="INSERT_ALL_COLUMNS"/>
	</sql>

	<insert id="insert" parameterType="titan.wms.common.dao.meta.inside.InventoryTransactionDO" useGeneratedKeys="true" keyProperty="id">
		insert into
		<include refid="TABLE_NAME" />
		(
		<include refid="INSERT_ALL_COLUMNS"/>
		)
		values (#{skuCode},#{attributeId},#{locationCode},#{containerCode},#{clientCode}
		,#{transactionType},#{subType},#{sourceOrderNo},#{sourceOrderLine},#{sourceOrderType},#{sourceTaskNo},#{sourceTaskLine},#{reason}
		,#{changeType},#{count},#{beforeCount},#{afterCount},#{operatorId},#{createTime})
	</insert>

</mapper>